{
	"extends": "../../tsconfig.base.json",
	"compilerOptions": {
		"types": ["unplugin-icons/types/react"],
		"jsx": "react-jsx",

		"paths": {
			"@components/*": ["./src/components/*"],
			"@contexts/*": ["./src/contexts/*"],
			"@hooks/*": ["./src/hooks/*"],
			"@lib/*": ["./src/lib/*"],
			"@/*": ["./src/*"]
