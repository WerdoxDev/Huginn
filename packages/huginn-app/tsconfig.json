{
   "extends": "../../tsconfig.base.json",
   "compilerOptions": {
      "types": ["unplugin-icons/types/react"],
      "jsx": "react-jsx",
      "preserveSymlinks": true,

      "paths": {
         "@components/*": ["./src/components/*"],
         "@contexts/*": ["./src/contexts/*"],
         "@hooks/*": ["./src/hooks/*"],
         "@lib/*": ["./src/lib/*"],
         "@/*": ["./src/*"]
      }
   },
   "include": ["src", "src-tauri", "auto-imports.d.ts", "index.d.ts"]
}
