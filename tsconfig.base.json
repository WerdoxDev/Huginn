{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"moduleDetection": "force",
		"lib": ["ESNext", "DOM", "DOM.Iterable"],
		"forceConsistentCasingInFileNames": true,
		"declaration": true,
		"declarationMap": true,
		"composite": true,
		"isolatedModules": true,
		"strict": true,
		"skipLibCheck": true
	}
}
